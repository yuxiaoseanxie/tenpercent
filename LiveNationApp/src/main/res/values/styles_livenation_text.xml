<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="ticketbar_options_text" parent="ln.text.tiny.white">
        <item name="android:textSize">13sp</item>
    </style>

    <style name="fragment_message_subject_text">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="fragment_message_details_text">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/livenation_gray</item>
    </style>

    <style name="ln.text.order_confirmation_button">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ln.text.order_confirmation_button.title">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="ln.text.order_confirmation_button.details">
        <item name="android:textSize">12sp</item>
    </style>


    <style name="bottom_bar_button_text">
        <item name="android:textSize">17sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@drawable/ui_bottom_bar_button_textcolor</item>
    </style>

    <style name="dialog_title">
        <item name="android:textColor">@color/ui_header1_text</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="list_help_menu_item_name_text">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="list_help_menu_item_description_text">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/livenation_gray</item>
    </style>

    <style name="calendar_dialog_cell_title">
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingBottom">@dimen/calendar_dialog_cell_title_padding_bottom</item>
    </style>

    <style name="calendar_dialog_cell_prefix">
        <item name="android:textColor">@color/ui_header1_text</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="calendar_dialog_cell_date">
        <item name="android:textColor">@color/livenation_gray</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="calendar_dialog_rating_description" parent="ln.text.medium">
        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="order_confirmation_header_text">
        <item name="android:textSize">15sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="order_confirmation_header_text.large">
        <item name="android:textSize">33sp</item>
    </style>

    <style name="order_confirmation_section_header_text">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/livenation_red</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="order_confirmation_detail_text">
        <item name="android:textSize">15sp</item>
    </style>

    <style name="order_confirmation_detail_text.label">
        <item name="android:textStyle">bold</item>
    </style>

    <style name="onboarding_title_text">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/livenation_red</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="onboarding_item_title_text">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="onboarding_item_description_text">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="onboarding_skip_text">
        <item name="android:textStyle">bold|italic</item>
        <item name="android:textColor">@drawable/clickable_text_selector</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="onboarding_scan_text">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>

    <style name="on_boarding_warning_text">
        <item name="android:textColor">@color/footer_text</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="text"/>

    <style name="rating_button" parent="android:Widget.Button"></style>

    <style name="megaticket_appearance" parent="ln.text.small.bold">
        <item name="android:textColor">@color/megaticket</item>
    </style>

</resources>