<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:minHeight="@dimen/list_show_item_min_height">

    <com.livenation.mobile.android.na.ui.views.VerticalDate
        android:id="@+id/list_generic_show_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/venue_show_date_margin_left"
        android:layout_marginRight="@dimen/venue_show_date_margin_right"
        android:layout_marginTop="@dimen/venue_show_date_margin_top" />
    />

    <TextView
        android:id="@+id/list_generic_show_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@id/list_generic_show_date"
        android:layout_toRightOf="@id/list_generic_show_date"
        android:text="@string/mock_artist_title_1"
        android:textAppearance="@style/list_show_title" />

    <TextView
        android:id="@+id/list_generic_show_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/list_generic_show_title"
        android:layout_below="@id/list_generic_show_title"
        android:text="@string/mock_venue_title_1"
        android:textAppearance="@style/list_show_location" />

</RelativeLayout>